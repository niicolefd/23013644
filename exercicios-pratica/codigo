#include <stdio.h>
#include <stdlib.h>

int main() {
    float n1, n2;
    
    printf("Digite dois numeros: ");
    scanf("%f %f", &n1, &n2);
    
    printf("Soma dos numeros: %f", n1+n2);
    printf("Subtração dos numeros: %f", n1-n2);
    printf("Divisao dos numeros: %f", n1/n2);
    printf("Multiplicacao dos numeros: %f", n1*n2);
    
    return 0;
}
