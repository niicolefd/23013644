#include <stdio.h>

int main() {
    int a = 5, b = 5, c, d, e;
    
    printf("** FASE 1: INCREMENTO SIMPLES** \n \n");
    printf("a = %d\n", a);
    printf("b = %d\n", b);
   
    a++;
    
    ++b;
    
    printf("a = %d\n", a);
    printf("b = %d\n \n", b);
    printf("** FASE 2: INCREMENTO COM ATRIBUICAO** \n \n");
    
    c = a++;
    
    d = ++b;
    
    printf("c = %d\n", c);
    printf("d = %d\n", d);
    printf("a = %d\n", a);
    printf("b = %d\n", b);
    
return 0;
}
