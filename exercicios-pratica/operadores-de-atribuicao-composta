#include <stdio.h>
#include <stdlib.h>

int main() {
    float a = 20.5;
    int b = 7;
    
    a -= b;
    
    printf("a = %.1f\n", a);
    
    a *= 4;
    
    printf("a = %.1f\n", a);
    
    a /= 3;
    
    printf("a = %.1f\n", a);
    
    a += 2;
    
    printf("a = %.1f\n", a);
    
    b %= 5;
    
    printf("b = %d\n", b);

return 0;
}
