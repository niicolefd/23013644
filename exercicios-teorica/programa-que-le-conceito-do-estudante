#include <stdio.h>
#include <ctype.h>

int main() {
    char conceito;
    
    printf("Informe o conceito do aluno: ");
    scanf("%c", &conceito);
    
    conceito = toupper(conceito);
    
    if (conceito == 'A') {
        printf("Excelente");
        }
    else if (conceito == 'B') { 
        printf("Otimo");
        }
    else if (conceito == 'C') {
        printf("Bom");
        }
    else if (conceito == 'D') {
        printf("Regular");
        }
    else if (conceito == 'E') {
        printf("Ruim");
        }
    else if (conceito == 'F') {
        printf("Tente novamente");
        }
    else {
        printf("Erro");
    }

    return 0;
}
